l)) {\n          next(endingHtmlUrl)\n        } else if (isRouteExists(router, endingSlashUrl)) {\n          next(endingSlashUrl)\n        } else {\n          next()\n        }\n      } else if (/\\/$/.test(to.path)) {\n        const endingHtmlUrl = to.path.replace(/\\/$/, '') + '.html'\n        if (isRouteExists(router, endingHtmlUrl)) {\n          next(endingHtmlUrl)\n        } else {\n          next()\n        }\n      } else {\n        next()\n      }\n    }\n  })\n}\n\nfunction isRouteExists (router, path) {\n  const pathLower = path.toLowerCase()\n  return router.options.routes.some(route => route.path.toLowerCase() === pathLower)\n}\n"],"mappings":"AAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAEA,OAAO,SAASA,0BAAT,CAAqCC,MAArC,EAA6C;EAClDA,MAAM,CAACC,UAAP,CAAkB,CAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,KAAoB;IACpC,IAAIC,aAAa,CAACL,MAAD,EAASE,EAAE,CAACI,IAAZ,CAAjB,EAAoC;MAClCF,IAAI;IACL,CAFD,MAEO;MACL,IAAI,CAAC,eAAeG,IAAf,CAAoBL,EAAE,CAACI,IAAvB,CAAL,EAAmC;QACjC,MAAME,cAAc,GAAGN,EAAE,CAACI,IAAH,GAAU,GAAjC;QACA,MAAMG,aAAa,GAAGP,EAAE,CAACI,IAAH,GAAU,OAAhC;;QACA,IAAID,aAAa,CAACL,MAAD,EAASS,aAAT,CAAjB,EAA0C;UACxCL,IAAI,CAACK,aAAD,CAAJ;QACD,CAFD,MAEO,IAAIJ,aAAa,CAACL,MAAD,EAASQ,cAAT,CAAjB,EAA2C;UAChDJ,IAAI,CAACI,cAAD,CAAJ;QACD,CAFM,MAEA;UACLJ,IAAI;QACL;MACF,CAVD,MAUO,IAAI,MAAMG,IAAN,CAAWL,EAAE,CAACI,IAAd,CAAJ,EAAyB;QAC9B,MAAMG,aAAa,GAAGP,EAAE,CAACI,IAAH,CAAQI,OAAR,CAAgB,KAAhB,EAAuB,EAAvB,IAA6B,OAAnD;;QACA,IAAIL,aAAa,CAACL,MAAD,EAASS,aAAT,CAAjB,EAA0C;UACxCL,IAAI,CAACK,aAAD,CAAJ;QACD,CAFD,MAEO;UACLL,IAAI;QACL;MACF,CAPM,MAOA;QACLA,IAAI;MACL;IACF;EACF,CAzBD;AA0BD;;AAED,SAASC,aAAT,CAAwBL,MAAxB,EAAgCM,IAAhC,EAAsC;EACpC,MAAMK,SAAS,GAAGL,IAAI,CAACM,WAAL,EAAlB;EACA,OAAOZ,MAAM,CAACa,OAAP,CAAeC,MAAf,CAAsBC,IAAtB,CAA2BC,KAAK,IAAIA,KAAK,CAACV,IAAN,CAAWM,WAAX,OAA6BD,SAAjE,CAAP;AACD"}]}